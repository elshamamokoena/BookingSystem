    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,500,600,700%7cPoppins:300,400,500,600,700,800,900&amp;display=swap" rel="stylesheet">
    <link href="_content/BookingSystem.Shared/vendors/simplebar/simplebar.min.css" rel="stylesheet">
    <link href="_content/BookingSystem.Shared/assets/css/theme-rtl.min.css" rel="stylesheet" id="style-rtl">
    <link href="_content/BookingSystem.Shared/assets/css/theme.min.css" rel="stylesheet" id="style-default">
    <link href="_content/BookingSystem.Shared/assets/css/user-rtl.min.css" rel="stylesheet" id="user-style-rtl">
    <link href="_content/BookingSystem.Shared/assets/css/user.min.css" rel="stylesheet" id="user-style-default">

    <link href="BookingSystem.Web.Wasm.styles.css" rel="stylesheet" />


    <script>
        var isRTL = JSON.parse(localStorage.getItem('isRTL'));
        if (isRTL) {
            var linkDefault = document.getElementById('style-default');
            var userLinkDefault = document.getElementById('user-style-default');
            linkDefault.setAttribute('disabled', true);
            userLinkDefault.setAttribute('disabled', true);
            document.querySelector('html').setAttribute('dir', 'rtl');
        } else {
            var linkRTL = document.getElementById('style-rtl');
            var userLinkRTL = document.getElementById('user-style-rtl');
            linkRTL.setAttribute('disabled', true);
            userLinkRTL.setAttribute('disabled', true);
        }
    </script>
    <HeadOutlet @rendermode="InteractiveServer"></HeadOutlet>
