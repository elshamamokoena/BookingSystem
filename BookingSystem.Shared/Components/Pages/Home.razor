@page "/"
@using BookingSystem.Shared.Components.Pages.Consumables.EditConsumablePage
@using BookingSystem.Shared.Components.Pages.Dashboard
@using Microsoft.AspNetCore.Authorization
@* @attribute [Authorize(Policy = AuthorizationPolicies.Policies.IsManager)] *@
<PageTitle>Home</PageTitle>

 <DashboardPage/> 

@code{
    [CascadingParameter] 
    public Task<AuthenticationState> AuthenticationStateTask { get; set; }
    protected override async Task OnInitializedAsync()
    {
        await LoadAuthenticationStateAsync();
        await base.OnInitializedAsync();
    }
    private async Task LoadAuthenticationStateAsync()
    {
        var user = (await AuthenticationStateTask).User;
    }
}