@page "/booking-list"
@inherits BaseComponent


<div class="card shadow-none">
    <div class="card-header">
        <div class="row flex-between-center">
            <div class="col-6 col-sm-auto d-flex align-items-center pe-0">
                <h5 class="fs-9 mb-0 text-nowrap py-2 py-xl-0">Bookings</h5>
            </div>
    
        </div>
    </div>
    <div class="card-body px-0 pt-0">








        <div class="table-list"  id="bookingListTable">
            <div class="table-responsive scrollbar mb-3">
                <table class="table table-sm table-striped fs-10 mb-0 overflow-hidden">
                    <thead class="bg-200">
                        <tr>
                            <th class="text-900 sort pe-1 align-middle white-space-nowrap" data-sort="bookingNumber">Booking</th>
                            <th class="text-900 sort pe-1 align-middle white-space-nowrap pe-7" data-sort="created">Created</th>
                            <th class="text-900 sort pe-1 align-middle white-space-nowrap" data-sort="event" style="min-width: 12.5rem;">Conference</th>
                            <th class="text-900 sort pe-1 align-middle white-space-nowrap text-center" data-sort="status">Status</th>
                            <th class="text-900 sort pe-1 align-middle white-space-nowrap text-end" data-sort="conferenceRoom">Room</th>
                            <th class="no-sort"></th>
                        </tr>
                    </thead>
                    <tbody class="list"></tbody>
                </table>
            </div>
            <div class="d-flex align-items-center justify-content-center">
                <button class="btn btn-sm btn-falcon-default me-1" type="button" title="Previous" data-list-pagination="prev"><span class="fas fa-chevron-left"></span></button>
                <ul class="pagination mb-0"></ul><button class="btn btn-sm btn-falcon-default ms-1" type="button" title="Next" data-list-pagination="next"><span class="fas fa-chevron-right"> </span></button>
            </div>
        </div>





















       
    </div>
</div>